{
    "builds": [
      {
        "src": "myproject_k/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      },
      {
        "src": "staticfiles/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/staticfiles/$1" },
      { "src": "/(.*)", "dest": "myproject_k/wsgi.py" }
    ]
  }
  